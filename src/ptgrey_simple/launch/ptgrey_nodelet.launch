<launch>
  <node pkg="nodelet" type="nodelet" name="standalone_nodelet"
args="manager" output="screen"/>

  <node pkg="nodelet" type="nodelet" name="ptgrey_simple" args="load
ptgrey_simple/ptgrey_simple standalone_nodelet" output="screen">
      <!-- if property is -1, it will not be set-->
      <param name="framerate" value="15.0" /> 
      <param name="shutter" value="3.5" /> 
  </node>
  
</launch>
